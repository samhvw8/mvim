{
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "css",
    "markdown",
    "Markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "codeLens.enable": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enableSign": true,
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshAfterSave": false,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualText": true,
  "json.enable": true,
  "json.format.enable": true,
  "explorer.keyMappings": {
    "i": false, // cancel default mapkey
    "gk": "expandablePrev",
    "gj": "expandableNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": [
      "expandable?",
      "expand",
      "open"
    ],
    "J": [
      "toggleSelection",
      "nodeNext"
    ],
    "K": [
      "toggleSelection",
      "nodePrev"
    ],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "<2-LeftMouse>": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "o": [
      "expanded?",
      "collapse",
      "expand"
    ],
    "<cr>": [
      "expandable?",
      "cd",
      "open"
    ],
    "e": "open",
    "s": "open:split",
    "S": "open:split:plain",
    "E": "open:vsplit",
    "t": "open:tab",
    "<bs>": "gotoParent",
    "gp": "preview:labeling",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
      "]]": "sourceNext",
      "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[c": "gitPrev",
          "]c": "gitNext",
          "<<": "gitStage",
          ">>": "gitUnstage"
  },
  "json.schemas": [
    {
      "fileMatch": [
        "bower.json",
        ".bower.json"
      ],
      "url": "http://schemastore.org/schemas/json/bower"
    },
    {
      "fileMatch": [
        "tsconfig.json"
      ],
      "url": "http://json.schemastore.org/tsconfig"
    },
    {
      "fileMatch": [
        "package.json"
      ],
      "url": "http://json.schemastore.org/package"
    }
  ],
  "python.analysis.logLevel": "Trace",
  "python.formatting.provider": "black",
  "python.jediEnabled": true,
  "python.jediPath": "/usr/local/lib/python3.7/site-packages",
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": true,
  "signature.enable": true,
  "signature.target": "echo",
  "suggest.echodocSupport": true,
  "tslint.autoFixOnSave": false,
  "tslint.jsEnable": true,
  "tslint.suppressWhileTypeErrorsPresent": false,
  "tsserver.log": "verbose",
  "tsserver.npm": "/usr/local/bin/npm",
  "tsserver.trace.server": "verbose",
  "yaml.completion": true,
  "yaml.format.bracketSpacing": true,
  "yaml.format.enable": true,
  "yaml.format.proseWrap": "always",
  "yaml.format.singleQuote": true,
  "yaml.hover": true,
  "yaml.schemaStore.enable": true,
  "yaml.validate": true,
  "yaml.schemas": {
    "http://json.schemastore.org/circleciconfig": "/.circleci/config.yml",
    "http://json.schemastore.org/huskyrc": "/.huskyrc.yml",
    "http://json.schemastore.org/travis": "/.travis.yml",
    "http://json.schemastore.org/stylelintrc": "/.stylelintrc.yml",
    "http://json.schemastore.org/eslintrc": "/.eslintrc.yml",
    "http://json.schemastore.org/gitlab-ci": "/.gitlab-ci.yml",
    "http://json.schemastore.org/prettierrc": "/.prettierrc.yml",
    "http://json.schemastore.org/ansible-stable-2.7": "/playbook/**/*.y*ml",
    "https://raw.githubusercontent.com/docker/compose/master/compose/config/config_schema_v3.8.json": "docker-compose*.y*ml"
    /* "kubernetes": "/*.y*ml", */
  },
  "diagnostic-languageserver.filetypes": {
    "zsh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "zsh": "shfmt"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": [
        "-i",
        "2",
        "-bn",
        "-ci",
        "-sr"
      ]
    }
  },
  "docker.enable": true,
  "languageserver": {},
  "cSpell.userWords": [
    "pymongo"
  ],
  "yank.enableCompletion": true,
  "yank.limit": 10,
  "yank.highlight.duration": 250,
  "yank.highlight.enable": true,
  "eslint.enable": true,
  "eslint.trace.server": "verbose",
  "eslint.autoFixOnSave": true,
  "eslint.autoFix": true,
  "git.enableGutters": false,
  "git.addedSign.hlGroup": "",
  "git.changedSign.hlGroup": "",
  "git.removedSign.hlGroup": "",
  "git.topRemovedSign.hlGroup": "",
  "git.changeRemovedSign.hlGroup": "",
}
